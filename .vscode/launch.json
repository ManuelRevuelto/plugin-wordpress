{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Listen for Xdebug (docker)",
            "type": "php",
            "request": "launch",
            "pathMappings": {
                "/var/www/html/wp-content/plugins/iu-coming-soon-page": "${workspaceRoot}/wp-content/plugins/iu-coming-soon-page"
            },
            "port": 9000
        },
        {
            "name": "Launch currently open script",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9000
        }
    ]
}